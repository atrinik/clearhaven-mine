# This Makefile will generate the zlib local library to be used by the
# client.

SRCS = \
	adler32.c \
	crc32.c \
	inffast.c \
	inflate.c \
	inftrees.c \
	uncompr.c \
	zutil.c \

OBJS = $(SRCS:.c=.o)

ARFLAGS = cq

AR = @AR@
RM = @RM@

RANLIB = @RANLIB@

DEPEND = @DEPEND@

CFLAGS = @CFLAGS@ $(EXTRA_CFLAGS)

INCLUDES= $(EXTRA_INCLUDES) -I./include

zlib.a: $(OBJS)
	$(RM) -f $@
	$(AR) $(ARFLAGS) $@ $(OBJS)
	$(RANLIB) $@

depend:
	$(DEPEND) $(DEPENDFLAGS) -- $(CFLAGS) $(INCLUDES) -- $(SRCS)

# Nothing to install here
install::

clean:
	$(RM) -f $(OBJS) Makefile.bak

distclean: clean
	$(RM) -f Makefile zlib.a

.c.o:
	$(CC) $(CFLAGS) $(INCLUDES)  -c $*.c

all:: zlib.a

